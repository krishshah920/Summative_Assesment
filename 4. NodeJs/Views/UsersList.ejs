<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="css/style.css">
    <title>List Of Users</title>
</head>
<body>

  
    <h4>Hello  </h4> 
    <h1>List of Users</h1>
   <center> <table id="table" class="table" >
        <thead>
            <tr>
                <th>Employee Id</th>
                <th>first Name</th>
                <th>last Name</th>
                <th>Username</th>
                <th>Password</th>
                <th>Address</th>
                <th>Contact</th>
            </tr>
        </thead>
         <tbody>
         <% print.forEach(function (User) { %>
            <tr>
                <td><%= User.EmployeeID %></td>
                <td><%= User.FirstName %></td>
                <td><%= User.LastName %></td>
                <td><%= User.username %></td>
                <td><%= User.Password %></td>
                <td><%= User.Address %></td>
                <td><%= User.Contact %></td>

            </tr>
         <% }) %>
         </tbody>
    </table>
    </center>
<form action="/UsersList" method="post">
   <button type="submit">Delete Record</button>
</form>
</body>
</html>